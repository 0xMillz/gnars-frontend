{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "yarn typechain && tsc && yarn migrate:prod ",
    "start": "node dist/index.js",
    "generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "migrate:prod": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "typechain": "typechain --target=ethers-v5 'abis/*.json' --out-dir src/types/contracts",
    "backfill": "ts-node ./src/scripts/backfill.ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@fastify/sensible": "^5.1.1",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@prisma/client": "^4.4.0",
    "@sinclair/typebox": "^0.24.44",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.1",
    "fastify": "^4.6.0",
    "prisma": "^4.4.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node": "^18.7.23",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4"
  }
}
